#!/bin/sh

set javaDir=/usr/java
set jmaxRootDir=/usr/local/max

$javaDir/bin/java -noasyncgc -mx64m -ms2m -classpath .:$javaDir/lib/rt.jar:$jmaxRootDir/java/classes/jmax.jar:$jmaxRootDir/java/lib/jacl1.0/jacl.jar:$jmaxRootDir/java/lib/swing-1.0.1/swingall.jar ircam.jmax.MaxApplication -root $jmaxRootDir $*

