include $(ROOTDIR)/Makefiles/Makefile.$(ARCH)

CC= cc
CFLAGS= $(ARCH_CFLAGS)

AR= ar
ARFLAGS= rc $(ARCH_ARFLAGS)

LIBDIR  = $(ROOTDIR)/lib/$(ARCH)
OBJDIR  = $(ROOTDIR)/obj/$(ARCH)

OBJECTS = $(addprefix $(OBJDIR)/, $(addsuffix .o , $(basename $(SOURCES))))









