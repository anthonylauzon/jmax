all:
	(cd control; $(MAKE)  all)
	(cd data; $(MAKE)  all)
	(cd filters; $(MAKE)  all)
	(cd io; $(MAKE)  all)
	(cd midi; $(MAKE)  all)
	(cd sampling; $(MAKE)  all)
	(cd pitch; $(MAKE)  all)
	(cd signal; $(MAKE)  all)
.PHONY: all

clean:
	(cd control; $(MAKE) clean)
	(cd data; $(MAKE) clean)
	(cd filters; $(MAKE) clean)
	(cd io; $(MAKE) clean)
	(cd midi; $(MAKE) clean)
	(cd sampling; $(MAKE) clean)
	(cd pitch; $(MAKE) clean)
	(cd signal; $(MAKE) clean)
.PHONY: clean
