#	(cd gist; $(MAKE) clean)
#	(cd iana; $(MAKE) clean)
	(cd icebrowser; $(MAKE) clean)
	(cd espresso; $(MAKE) clean)
	(cd table; $(MAKE) clean)

clean_c:
	(cd fts1.5; $(MAKE) clean)
#	(cd gist; $(MAKE) clean)
#	(cd iana; $(MAKE) clean)

clean_java:
	(cd icebrowser; $(MAKE) clean)
	(cd espresso; $(MAKE) clean)
	(cd table; $(MAKE) clean)

