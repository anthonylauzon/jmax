all:
	(cd fts1.5; $(MAKE) -k all)
#	(cd gist; $(MAKE) -k all)
#	(cd iana; $(MAKE) -k all)
	(cd icebrowser; $(MAKE) -k all)
	(cd espresso; $(MAKE) -k all)
.PHONY: all

clean:
	(cd fts1.5; $(MAKE) clean)
#	(cd gist; $(MAKE) clean)
#	(cd iana; $(MAKE) clean)
	(cd icebrowser; $(MAKE) clean)
	(cd espresso; $(MAKE) clean)
.PHONY: clean
